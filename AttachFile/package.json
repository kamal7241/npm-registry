{
    "name": "@t2/attach-file",
    "version": "0.2.24",
    "private": false,
    "main": "./src/index.js",
    "module": "dist/AttachFile.esm.js",
    "unpkg": "dist/AttachFile.min.js",
    "browser": {
        "./sfc": "src/components/AttachFile.vue"
    },
    "author": {
        "name": "Front-End Team"
    },
    "files": [
        "src/*",
        "*.json",
        "*.js",
        "*.scss"
    ],
    "scripts": {
        "dev": "node build/dev-server.js",
        "serve": "vue-cli-service serve",
        "lint": "vue-cli-service lint",
        "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
        "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/AttachFile.umd.js",
        "build:es": "rollup --config build/rollup.config.js --format es --file dist/AttachFile.esm.js",
        "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/AttachFile.min.js"
    },
    "dependencies": {
        "@types/file-saver": "^2.0.1",
        "axios": "^0.19.0",
        "core-js": "^2.6.5",
        "file-saver": "^2.0.2",
        "rollup": "^1.20.2",
        "rollup-plugin-buble": "^0.19.8",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-vue": "^5.1.2",
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.11.0",
        "@vue/cli-plugin-eslint": "^3.11.0",
        "@vue/cli-service": "^3.11.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}