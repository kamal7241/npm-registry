{
  "name": "@t2/date-picker",
  "version": "0.1.3",
  "private": false,
  "main": "dist/DatePicker.umd.js",
  "module": "dist/DatePicker.esm.js",
  "unpkg": "dist/DatePicker.min.js",
  "browser": {
    "./sfc": "src/components/DatePicker.vue"
  },
  "author": {
    "name": "Front-End Team"
  },
  "files": [
    "src/*",
    "dist/*",
    "*.json",
    "*.js",
    "*.scss"
  ],
  "scripts": {
    "dev": "node build/dev-server.js",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/DatePicker.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/DatePicker.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/DatePicker.min.js"
  },
  "dependencies": {
    "@revam/rollup-plugin-common": "^1.2.1",
    "accounting": "^0.4.1",
    "core-js": "^2.6.5",
    "moment": "^2.24.0",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "stylus-loader": "^3.0.2",
    "vee-validate": "^2.2.13",
    "vue": "^2.6.10",
    "vue-events": "^3.1.0",
    "vue-js-modal": "^1.3.31",
    "vue-scroll-range-datepicker": "^3.0.6",
    "vue-smooth-picker": "^0.4.2",
    "vueble": "^0.1.3",
    "vuejs-datepicker": "^1.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10",
    "vuetable-2": "^1.7.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}