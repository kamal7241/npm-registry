{
  "name": "@t2/hijri-meladi-picker",
  "version": "0.2.4",
  "private": false,
  "author": {
    "name": "Front-End Team"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "lint": "vue-cli-service lint",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/HGDatePicker.esm.js",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/HGDatePicker.umd.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/HGDatePicker.min.js",
    "dev": "node build/dev-server.js"
  },
  "main": "dist/HGDatePicker.umd.js",
  "module": "dist/HGDatePicker.esm.js",
  "browser": {
    "./sfc": "src/components/HGDatePicker.vue"
  },
  "unpkg": "dist/HGDatePicker.min.js",
  "files": [
    "src/*",
    "dist/*",
    "*.json",
    "*.js",
    "*.scss"
  ],
  "dependencies": {
    "@t2/vue-hijri-calander": "^0.3.4",
    "core-js": "^2.6.5",
    "moment": "^2.24.0",
    "moment-hijri": "^2.1.2",
    "rollup": "^1.19.4",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-datepicker-local": "^1.0.19"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
